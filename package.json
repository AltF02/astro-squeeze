{
  "name": "astro-squeeze",
  "version": "0.2.2-0",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "description": "Fast astro compressor written in rust",
  "repository": {
    "url": "https://github.com/AltF02/astro-squeeze"
  },
  "keywords": [
    "Rust",
    "astro-plugin",
    "brotli"
  ],
  "napi": {
    "name": "astro-squeeze",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "EUPL-1.2",
  "devDependencies": {
    "@napi-rs/cli": "^2.17.0",
    "astro": "^5.3.1",
    "vitest": "^3.0.7"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --js bindings.cjs --dts bindings.d.ts --platform --release",
    "build:debug": "napi build --js bindings.cjs --dts bindings.d.ts --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "universal": "napi universal",
    "version": "napi version"
  },
  "dependencies": {
    "yarn": "^1.22.22"
  }
}
