{
  "name": "astro-squeeze",
  "version": "0.1.2",
  "main": "index.js",
  "type": "module",
  "types": "index.d.ts",
  "description": "Fast astro compressor written in rust",
  "keywords": [
    "Rust",
    "astro-plugin",
    "brotli"
  ],
  "napi": {
    "name": "astro-squeeze",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-unknown-linux-gnu",
        "i686-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "license": "EUPL-1.2",
  "devDependencies": {
    "@napi-rs/cli": "^2.17.0",
    "astro": "^5.3.1",
    "vitest": "^3.0.7"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --js bindings.cjs --dts bindings.d.ts --platform --release",
    "build:debug": "napi build --js bindings.cjs --dts bindings.d.ts --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "universal": "napi universal",
    "version": "napi version"
  },
  "dependencies": {
    "yarn": "^1.22.22"
  },
  "optionalDependencies": {
    "astro-squeeze-win32-x64-msvc": "0.1.2",
    "astro-squeeze-darwin-x64": "0.1.2",
    "astro-squeeze-linux-x64-gnu": "0.1.2",
    "astro-squeeze-linux-x64-musl": "0.1.2",
    "astro-squeeze-linux-arm64-gnu": "0.1.2",
    "astro-squeeze-win32-ia32-msvc": "0.1.2",
    "astro-squeeze-linux-arm-gnueabihf": "0.1.2",
    "astro-squeeze-darwin-arm64": "0.1.2",
    "astro-squeeze-android-arm64": "0.1.2",
    "astro-squeeze-freebsd-x64": "0.1.2",
    "astro-squeeze-linux-arm64-musl": "0.1.2",
    "astro-squeeze-win32-arm64-msvc": "0.1.2",
    "astro-squeeze-android-arm-eabi": "0.1.2"
  }
}